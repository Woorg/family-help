.in
	margin auto
	padding-left 10px
	padding-left ratio-percentage(10,1200)
	padding-right 10px
	padding-right ratio-percentage(10,1200)
	width 1200px
	// min-width 290px
	// max-width 1200px
	+below(1180px)
		width 100%


.wysiwyg
	h1
		color #39434b
		font-family $roboto-bld
		font-size 22px
	p
		margin-bottom 36px
		line-height 30px
		line-height em(30px,16px)
	img
		max-width 100%
		height auto
		display block

.title
	margin-bottom 16px
	color #39434b
	font-family $roboto-bld
	font-size 22px
	line-height 30px
	text-transform uppercase

.text
	@extends .wysiwyg
